#include<iostream>

using namespace std;
 
void repeat(int a[], int n)
{
    int i, j;
    cout<<"\n Repeating Elements are : ";
    for(i = 0; i < n; i++)
    
        for(j = i + 1; j < n; j++)
        
            if(a[i] == a[j])
            
                cout << a[i] << " ";
}
 
 
int main()
{
    int a[] = {1,3,5,7,6,9,2,3,5,8,3};
    
    int n = sizeof(a)/sizeof(a[0]);
    
    repeat(a, n);
}
  
