#include<iostream>
#include<map>

using namespace std;

void twice(int a[], int n)
{
    map<int, int> h;
    for(int i = 0; i < n; i++)
    {
        h[a[i]]++;
    }
    for(int i = 1; i < n+1; i++)
    {
        if(h[i] > n/3)
        {
            cout<<"\n The number occuring more than n/3 : "<<i;
        }
    }

}

int main()
{
    int i,n,a;
    cout<<"size of array";
    cin>>n;
    for(i=0;i<n;i++;
    {
     cin>>a[i];
    }
    twice(a,n);
}
