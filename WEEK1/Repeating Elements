#include<iostream>

using namespace std;
 
void repeating(int a[], int n)
{
    int i, j;
    cout<<"\n Repeating Elements are : ";
    for(i = 0; i < n; i++)
    
        for(j = i + 1; j < n; j++)
        
            if(a[i] == a[j])
            
                cout << a[i] << " ";
}
 
 
int main()
{
    int n,i,a[100];
    cout<<"size of array";
    cin>>n;
    for(i=0;i<n;i++)
    {
        cin>>a[i];
    }
    
    
    repeating(a,n);
 }
