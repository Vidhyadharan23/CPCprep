#include <bits/stdc++.h>
using namespace std;
 

int Missing(int arr[], int n)
{
 
    int sum = (n + 1) * (n + 2) / 2;
    for (int i = 0; i < n; i++)
        sum -= arr[i];
    return sum;
}
 

int main()
{
   int arr[]={6,4,2,1,5};
   int n = sizeof(arr)/sizeof(arr[0]);
    
    int miss = Missing(arr, n);
    cout << "The missing number is "<<miss;
    
    return 0;
}
