#include <bits/stdc++.h>
using namespace std;


int missing( int arr[], int n){

    int ans = 0;
    
    for (int i = 0; i < n; i++)    
        ans = ans ^ arr[i];

    for (int i = 1; i <= n+1; i++)    
        ans = ans ^ i;
     
    return ans;
}



int main()
{
	int arr[] = {5,3,1,4,6};

	int n = sizeof(arr) / sizeof(arr[0]);

	cout<<"The missing number is "<<missing(arr,n);

	return 0;
}
